{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "**/*.ts",
        "**/*.tsx",
        "!node_modules/**"
    ],
    "compileOnSave": true,
    "buildOnSave": false,
    "files": [
        "src/app.ts",
        "src/controllers.ts",
        "src/directives.ts",
        "src/examin/controllers.ts",
        "src/examin/models.ts",
        "src/examin/services.ts",
        "typings/angular-ui-router/angular-ui-router.d.ts",
        "typings/angularjs/angular.d.ts",
        "typings/cordova-ionic/cordova-ionic.d.ts",
        "typings/cordova-ionic/plugins/keyboard.d.ts",
        "typings/cordova/cordova.d.ts",
        "typings/cordova/plugins/BatteryStatus.d.ts",
        "typings/cordova/plugins/Camera.d.ts",
        "typings/cordova/plugins/Contacts.d.ts",
        "typings/cordova/plugins/Device.d.ts",
        "typings/cordova/plugins/DeviceMotion.d.ts",
        "typings/cordova/plugins/DeviceOrientation.d.ts",
        "typings/cordova/plugins/Dialogs.d.ts",
        "typings/cordova/plugins/FileSystem.d.ts",
        "typings/cordova/plugins/FileTransfer.d.ts",
        "typings/cordova/plugins/Globalization.d.ts",
        "typings/cordova/plugins/InAppBrowser.d.ts",
        "typings/cordova/plugins/Keyboard.d.ts",
        "typings/cordova/plugins/Media.d.ts",
        "typings/cordova/plugins/MediaCapture.d.ts",
        "typings/cordova/plugins/NetworkInformation.d.ts",
        "typings/cordova/plugins/Push.d.ts",
        "typings/cordova/plugins/Splashscreen.d.ts",
        "typings/cordova/plugins/StatusBar.d.ts",
        "typings/cordova/plugins/Vibration.d.ts",
        "typings/cordova/plugins/WebSQL.d.ts",
        "typings/ionic/ionic.d.ts",
        "typings/jquery/jquery.d.ts",
        "typings/tsd.d.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
